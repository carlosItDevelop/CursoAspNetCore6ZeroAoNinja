
@model Cooperchip.ITDeveloper.Application.ViewModels.TagsViewModel

@{
    ViewData["Title"] = "Exclusão de Tags";
}

@*<vc:cabecalho titulo="Cooperchip-Sys / Módulo Tags"
              subtitulo="Administração de Tags"></vc:cabecalho>*@

<div class="panel panel-default panel-controls">
    <div class="panel panel-heading">
        <h2>Tem certeza que deseja exclir este registro?</h2>
    </div>

    <div class="panel panel-body">

        <div>
            <dl class="row">

                <partial name="Partial/_delete_details" />


            </dl>

            <form asp-action="Delete">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="Excluir" class="btn btn-danger" />
                <a class="btn btn-default" asp-action="Index">Listagem</a>
            </form>
        </div>
    </div>
</div>