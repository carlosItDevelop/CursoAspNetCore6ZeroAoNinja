@model Cooperchip.ITDeveloper.Application.ViewModels.TagsViewModel


<dt class="col-sm-2">
    @Html.DisplayNameFor(model => model.Tag)
</dt>
<dd class="col-sm-10">
    @Html.DisplayFor(model => model.Tag)
</dd>



<dt class="col-sm-2">
    @Html.DisplayNameFor(model => model.Note)
</dt>

<dd class="col-sm-10">

    <div class="panel-body">
        <div class="form-group">
            <div>
                <textarea class="form-control" id="summernote">
                                @Html.DisplayFor(model => model.Note)
                </textarea>
            </div>
        </div>
    </div>

</dd>


<br />


<dt class="col-sm-2">
    Source
</dt>
<dd class="col-sm-10">
    <a style="text-decoration: none; color: #ed5353" href="@Html.DisplayFor(model => model.SourceAt)" target="_blank">Acessar a Origem</a>
</dd>
<dt class="col-sm-2">
    @Html.DisplayNameFor(model => model.Author)
</dt>
<dd class="col-sm-10">
    @Html.DisplayFor(model => model.Author.Name)&nbsp;@Html.DisplayFor(model => model.Author.LastName)
</dd>